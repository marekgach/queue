{
    "name": "kurzor/queue",
    "description": "Implementation of PHP task queue using db to store tasks and statistics",
    "type": "library",
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "phpunit/dbunit": "1.4.1",
        "phpunit/phpunit-story": "^1.0",
        "phpunit/phpunit-selenium": "^1.4",
        "phpunit/php-invoker": "^1.1"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Marek Gach",
            "email": "gach@kurzor.net",
            "role": "lead"
        }
    ],
    "autoload": {
        "psr-0": {"Kurzor":"src/"}
    },
    "bin": ["bin/queue", "bin/queue.php"],
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml", ".travis.yml", "build.xml", "composer.phar", "*.swp", "*coveralls.yml"]
    }
}
